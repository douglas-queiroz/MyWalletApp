
CREATE TABLE Currency (
    id TEXT NOT NULL,
    name TEXT,
    symbol TEXT,
    created_at TEXT NOT NULL,
    updated_at TEXT NOT NULL
);

insert:
INSERT INTO Currency(
    id, name, symbol, created_at, updated_at
) VALUES (
    ?, ?, ?, ?, ?
);

getAll:
SELECT * FROM Currency;


deleteAll:
DELETE FROM Currency;