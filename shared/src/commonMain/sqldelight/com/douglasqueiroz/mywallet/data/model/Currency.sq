
CREATE TABLE CurrencyEntity (
    id TEXT NOT NULL,
    name TEXT,
    symbol TEXT,
    createdAt TEXT NOT NULL,
    updatedAt TEXT NOT NULL
);

insert:
INSERT INTO CurrencyEntity(
    id, name, symbol, createdAt, updatedAt
) VALUES (
    ?, ?, ?, ?, ?
);

getAll:
SELECT * FROM CurrencyEntity;


deleteAll:
DELETE FROM CurrencyEntity;