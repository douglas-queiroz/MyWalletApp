
CREATE TABLE CurrencyQuotationEntity (
    id TEXT NOT NULL,
    currencyConversionId TEXT NOT NULL,
    date TEXT,
    price REAL AS Float,
    createdAt TEXT NOT NULL,
    updatedAt TEXT NOT NULL
);

insert:
INSERT INTO CurrencyQuotationEntity(
    id, currencyConversionId, date, price , createdAt, updatedAt
) VALUES (
    ?, ?, ?, ?, ?, ?
);

deleteAll:
DELETE FROM CurrencyQuotationEntity;