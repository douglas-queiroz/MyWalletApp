
CREATE TABLE QuoteEntity (
    id TEXT NOT NULL,
    date TEXT,
    price REAL AS Float,
    shareId TEXT NOT NULL,
    createdAt TEXT NOT NULL,
    updatedAt TEXT NOT NULL
);

insert:
INSERT INTO QuoteEntity(
    id, date, price, shareId, createdAt, updatedAt
) VALUES (
    ?, ?, ?, ?, ?, ?
);

deleteAll:
DELETE FROM QuoteEntity;