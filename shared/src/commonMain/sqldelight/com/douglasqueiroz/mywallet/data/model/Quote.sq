
CREATE TABLE Quote (
    id INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL,
    date TEXT,
    price REAL AS Float,
    share_id INTEGER NOT NULL,
    created_at TEXT NOT NULL,
    updated_at TEXT NOT NULL
);

insert:
INSERT INTO Quote(
    id, date, price, share_id, created_at, updated_at
) VALUES (
    ?, ?, ?, ?, ?, ?
);